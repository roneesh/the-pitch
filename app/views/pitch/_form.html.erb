<!-- Pitch form_for Partial -->

<%= form_for @pitch, :html => { :multipart => true } do |f| %>

	<%= f.label :headline %>
	<%= f.text_field :headline %>
	<!-- Must eventually be a collection select or auto-populate -->
	<%= f.label :job_id %>
	<%= f.text_field :job_id %>

	<%= f.label :user_id %>
	<%= f.text_field :user_id %>

  	<%= f.file_field :profilepic %>

	<%= f.submit %>
<%end%>