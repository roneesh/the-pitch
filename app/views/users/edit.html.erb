<h1>Edit User <%= @user.id %></h1>

<%= form_for(@user, :url => "/user/#{@user.id}") do |f| %>

	<%= f.label :first_name%>
  	<%= f.text_field :first_name%>
    
  	<%= f.label :last_name%>
    <%= f.text_field :last_name%>

 	<%= f.label :email%>
    <%= f.text_field :email%>

   	<%= f.submit %>
    <%= link_to "or Cancel", users_path %>

<% end%>